      </Annotations>
      <Annotations Target="sap.s4.A_ProductPurchaseText/Language">
        <Annotation Term="Common.Label" String="Language Key"/>
        <Annotation Term="Core.Description" String="The language key indicates&#xA;&#xA;- the language in which texts are displayed,&#xA;- the language in which you enter texts,&#xA;- the language in which the system prints texts."/>
      </Annotations>
      <Annotations Target="sap.s4.A_ProductPurchaseText/LongText">
