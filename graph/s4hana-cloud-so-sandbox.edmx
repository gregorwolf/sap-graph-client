      <Annotations Target="sap.s4.A_BPFiscalYearInformation/BPBalSheetTotalAmtInBalShtCrcy">
        <Annotation Term="Common.Label" String="Balance Sheet Total"/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFiscalYearInformation/BPNumberOfEmployees">
        <Annotation Term="Common.Label" String="Number of Employees"/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFiscalYearInformation/BPCptlReserveAmtInBalShtCrcy">
