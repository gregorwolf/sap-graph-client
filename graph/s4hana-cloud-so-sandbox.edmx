      </Annotations>
      <Annotations Target="sap.s4.A_BPFiscalYearInformation/BPEquityCapitalAmtInBalShtCrcy">
        <Annotation Term="Common.Label" String="Equity capital"/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFiscalYearInformation/BPGrossPremiumAmtInBalShtCrcy">
        <Annotation Term="Common.Label" String="Gross Premium"/>
        <Annotation Term="Core.Description" String="Indicates the gross contribution of insurance premiums collected, taken from the profit and loss statement of an insurance company."/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFiscalYearInformation/BPNetPremiumAmtInBalShtCrcy">
        <Annotation Term="Common.Label" String="Net Premium"/>
        <Annotation Term="Core.Description" String="Indicates the net contribution of insurance premiums collected shown in the profit and loss statement of an insurance company.&#xA;&#xA;The net contribution is calculated by deducting the expenses from the insurance premiums collected."/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFiscalYearInformation/BPAnnualSalesAmtInBalShtCrcy">
        <Annotation Term="Common.Label" String="Annual sales"/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFiscalYearInformation/BPAnnualNetIncAmtInBalShtCrcy">
        <Annotation Term="Common.Label" String="Annual Net Income"/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFiscalYearInformation/BPDividendDistrAmtInBalShtCrcy">
        <Annotation Term="Common.Label" String="Dividend"/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFiscalYearInformation/BPDebtRatioInYears">
        <Annotation Term="Common.Label" String="Debt ratio"/>
        <Annotation Term="Core.Description" String="Specifies the number of years within which the outside capital can be repaid by the cashflow."/>
      </Annotations>
