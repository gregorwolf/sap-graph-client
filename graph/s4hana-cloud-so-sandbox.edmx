      </Annotations>
      <Annotations Target="sap.s4.A_BPFiscalYearInformation/BPLglRevnRsrvAmtInBalShtCrcy">
        <Annotation Term="Common.Label" String="Legal Revenue Res."/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFiscalYearInformation/RevnRsrvOwnStkAmtInBalShtCrcy">
        <Annotation Term="Common.Label" String="Rev.Reserves.OwnSt."/>
        <Annotation Term="Core.Description" String="Amount of revenue reserves for the company's own stock."/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFiscalYearInformation/BPStatryReserveAmtInBalShtCrcy">
