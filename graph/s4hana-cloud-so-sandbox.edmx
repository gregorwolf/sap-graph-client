      <Annotations Target="sap.s4.A_BPCreditWorthiness/BusinessPartnerOathDate">
        <Annotation Term="Common.Label" String="Date of affidavit"/>
        <Annotation Term="Core.Description" String="Date on which business partner made affidavit."/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPCreditWorthiness/BusinessPartnerIsBankrupt">
        <Annotation Term="Common.Label" String="Bankruptcy Proceed."/>
        <Annotation Term="Core.Description" String="Indicates that bankruptcy proceedings have been initiated against the business partner."/>
