      <Annotations Target="sap.s4.A_BPCreditWorthiness/BusinessPartnerBankruptcyDate">
        <Annotation Term="Common.Label" String="Bankruptcy Pr. Date"/>
        <Annotation Term="Core.Description" String="Date on which bankruptcy proceedings were instituted."/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPCreditWorthiness/BPForeclosureIsInitiated">
        <Annotation Term="Common.Label" String="Foreclosure"/>
        <Annotation Term="Core.Description" String="Indicates that foreclosure proceedings have been initiated for the business partner's assets."/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPCreditWorthiness/BPForeclosureDate">
        <Annotation Term="Common.Label" String="Date of foreclosure"/>
        <Annotation Term="Core.Description" String="Date on which foreclosure proceedings were initiated against the business partner."/>
      </Annotations>
