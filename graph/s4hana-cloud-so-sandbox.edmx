      <Annotations Target="sap.s4.A_CustomerReturnProcessStep/RetsMgmtProcessItem">
        <Annotation Term="Common.Label" String="Item No. in Process"/>
        <Annotation Term="Core.Description" String="Specifies an item in the start document of a returns process. This can be a returns order, a returns purchase order, or a returns stock transport order."/>
      </Annotations>
      <Annotations Target="sap.s4.A_CustomerReturnProcessStep/RetsMgmtProcItmQtySplit">
        <Annotation Term="Common.Label" String="Split ID"/>
        <Annotation Term="Core.Description" String="Identifies partial qantities of a specific returns item.&#xA;&#xA;The quantity of a specific returns item may be split up during the returns process, for example, because partial quantities of the returns item were assigned to different follow-up activities. These partial quantities are identified internally with a split ID."/>
      </Annotations>
      <Annotations Target="sap.s4.A_CustomerReturnProcessStep/RetsMgmtProcessStep">
        <Annotation Term="Common.Label" String="Step ID"/>
        <Annotation Term="Core.Description" String="Describes a given document, such as a returns order, an inbound delivery, or a purchase order, in the advanced returns process. Table MSR_I_STEP contains all valid step IDs.&#xA;&#xA;The advanced returns sequence Customer return received directly comprises the following steps:&#xA;0001 Create Returns Order&#xA;0021 Inbound Delivery&#xA;0031 Goods Receipt&#xA;0041 Inspection"/>
      </Annotations>
      <Annotations Target="sap.s4.A_CustomerReturnProcessStep/ReturnsDocumentType">
        <Annotation Term="Common.Label" String="Document Type"/>
        <Annotation Term="Core.Description" String="Specifies the returns document type, such as a returns order, goods movement, or credit memo."/>
      </Annotations>
      <Annotations Target="sap.s4.A_CustomerReturnProcessStep/ReturnsDocument">
        <Annotation Term="Common.Label" String="Document Number"/>
        <Annotation Term="Core.Description" String="Specifies the document number of a returns document."/>
      </Annotations>
      <Annotations Target="sap.s4.A_CustomerReturnProcessStep/ReturnsDocumentItem">
        <Annotation Term="Common.Label" String="Document Item"/>
        <Annotation Term="Core.Description" String="Specifies an item of the displayed document."/>
      </Annotations>
      <Annotations Target="sap.s4.A_CustomerReturnProcessStep/ReturnsDocumentStatus">
        <Annotation Term="Common.Label" String="Item Status"/>
        <Annotation Term="Core.Description" String="Specifies the status of the document item.&#xA;&#xA;A document item can have the following statuses:&#xA;Active&#xA;The document item has been created without errors.&#xA;Deletion Flag Set&#xA;The document item has been deleted.&#xA;Document Was Cancelled&#xA;The document item has been cancelled and a corresponding reversal or cancellation document exists.&#xA;Reversal (Cancellation) Document&#xA;This is a cancellation or reversal document and a corresponding cancelled document exists."/>
      </Annotations>
      <Annotations Target="sap.s4.A_CustomerReturnProcessStep/ReturnsDocumentApprovalStatus">
        <Annotation Term="Common.Label" String="Approval Status"/>
