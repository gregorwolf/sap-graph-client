      <Annotations Target="sap.s4.A_BPFinancialServicesExtn/FactoryCalendar">
        <Annotation Term="Common.Label" String="Factory calendar"/>
        <Annotation Term="Core.Description" String="Indicates the factory calendar used by the company or organization.&#xA;&#xA;All the customary holidays and rules in relation to holidays are defined in the standard system. You can change the default settings or add other factory calendars in Customizing under SAP NetWeaver -> General Settings -> Maintain Calendar."/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFinancialServicesExtn/BusinessPartnerOfficeCountry">
        <Annotation Term="Common.Label" String="C/R of Reg. Office"/>
        <Annotation Term="Core.Description" String="Indicates the country or region in which the business partner belonging to the category Organization has its registered office.&#xA;&#xA;The country/region key characteristic in limit management and the country/region of origin in the determination of the account assignment reference refer to this field in the case of organizations."/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFinancialServicesExtn/BusinessPartnerOfficeRegion">
        <Annotation Term="Common.Label" String="Region"/>
        <Annotation Term="Core.Description" String="Indicates the region in which the registered office of the business partner belonging to the category organization is located.&#xA;&#xA;Permitted regions can include federal states, provinces or counties, for example."/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFinancialServicesExtn/BPRegisteredOfficeName">
        <Annotation Term="Common.Label" String="Registered Office"/>
        <Annotation Term="Core.Description" String="Indicates the city in which the business partner belonging to the category organization is entered in the commercial register.&#xA;&#xA;The registered office determines the place of jurisdiction, or the area served by the court, where legal action may be taken against the business partner."/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFinancialServicesExtn/BPBalanceSheetCurrency">
        <Annotation Term="Common.Label" String="Bal. Sheet Currency"/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFinancialServicesExtn/BPLastCptlIncrAmtInBalShtCrcy">
        <Annotation Term="Common.Label" String="Capital increase"/>
        <Annotation Term="Core.Description" String="Indicates the amount by which the equity capital of the organization was increased."/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFinancialServicesExtn/BPLastCapitalIncreaseYear">
        <Annotation Term="Common.Label" String="Year"/>
        <Annotation Term="Core.Description" String="Indicates the year in which the equity capital of the organization was last increased."/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFinancialServicesExtn/BPBalanceSheetDisplayType">
        <Annotation Term="Common.Label" String="Bal.Sheet Disp."/>
