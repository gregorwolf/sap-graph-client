      <Annotations Target="sap.s4.A_BPFinancialServicesExtn/BPNumberOfHouseholdMembers">
        <Annotation Term="Common.Label" String="Household Members"/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFinancialServicesExtn/BPAnnualNetIncAmtInIncomeCrcy">
        <Annotation Term="Common.Label" String="Net Annual Income"/>
        <Annotation Term="Core.Description" String="Indicates the business partner's annual net income&#xA;&#xA;The net income is calculated on the basis of gross income minus direct taxes and social insurance contributions."/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFinancialServicesExtn/BPMonthlyNetIncAmtInIncomeCrcy">
        <Annotation Term="Common.Label" String="Monthly Net Income"/>
        <Annotation Term="Core.Description" String="Indicates the disposable monthly income of a business partner. The net income is calculated on the basis of gross income minus direct taxes and social insurance contributions."/>
      </Annotations>
      <Annotations Target="sap.s4.A_BPFinancialServicesExtn/BPAnnualNetIncomeYear">
