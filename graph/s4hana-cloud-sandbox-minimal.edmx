      <Annotations Target="sap.s4.A_SalesOrder/SalesDocApprovalStatus">
        <Annotation Term="Common.Label" String="Approval Status"/>
        <Annotation Term="Core.Computed" Bool="true"/>
        <Annotation Term="Core.Description" String="States whether a sales document is relevant for approvals and, if so, which approval stage it is in."/>
      </Annotations>
      <Annotations Target="sap.s4.A_SalesOrder/OverallSDProcessStatus">
        <Annotation Term="Common.Label" String="Overall Status"/>
        <Annotation Term="Core.Computed" Bool="true"/>
        <Annotation Term="Core.Description" String="The processing status of the entire sales document.&#xA;&#xA;The system determines the status of the entire sales document by taking into account the status of all header-related information and the status of all individual items. The status message tells you whether processing of the sales document is open, in progress, or complete."/>
      </Annotations>
      <Annotations Target="sap.s4.A_SalesOrder/TotalCreditCheckStatus">
